{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Central user JSON schema",
  "type": "object",
  "properties": {
    "_id": {
      "type": "string"
    },
    "remoteId": {
      "type": "number"
    },
    "firstName": {
      "type": "string"
    },
    "lastName": {
      "type": "string"
    },
    "enriched": {
      "type": "string"
    },
    "email": {
      "type": "string"
    },
    "phone": {
      "type": "string"
    },
    "gender": {
      "type": "string"
    }
  },
  "required": [
    "_id",
    "remoteId",
    "firstName",
    "lastName",
    "enriched"
  ]
}